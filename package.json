{
    "name": "{{repo_name}}",
    "version": "1.0.0",
    "description": "ts库项目模板",
    "publishConfig": {
        "access": "public"
    },
    "repository": "{{repo_url}}",
    "scripts": {
        "build": "rimraf dist && dts-bundle-generator -o dist/{{repo_name}}.d.ts index.ts  && vite build",
        "semantic-release": "semantic-release"
    },
    "keywords": [],
    "type": "module",
    "files": [
        "dist"
    ],
    "typings": "./dist/{{repo_name}}.d.ts",
    "module": "./dist/{{repo_name}}.es.js",
    "exports": {
        ".": {
            "types": "./dist/{{repo_name}}.d.ts",
            "import": "./dist/{{repo_name}}.es.js"
        }
    },
    "author": "{{author}}",
    "license": "MIT",
    "devDependencies": {
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^10.0.4",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^9.0.6",
        "@semantic-release/npm": "^11.0.0",
        "@semantic-release/release-notes-generator": "^11.0.7",
        "@types/node": "^20.6.2",
        "dts-bundle-generator": "^8.1.2",
        "handlebars": "^4.7.8",
        "rimraf": "^5.0.1",
        "semantic-release": "^22.0.0",
        "typescript": "^5.2.2",
        "vite": "^4.5.0"
    }
}
